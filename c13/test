echo "--------- CHECK, COMMPILE AND RUN" $1

echo "---------- MOULINETTE"

mv ex* ../

norminette -RCheckForbiddenSourceHeader "$1" | cat

echo "---------- GCC"

gcc -Wall -Wextra -Werror "$1" main.c | cat

echo "---------- CHMOD"

chmod +x a.out

echo "---------- EXECUTE"

rm -rf *.out
rm -rf */*.out
